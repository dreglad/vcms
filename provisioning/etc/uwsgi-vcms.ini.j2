[uwsgi]

processes = 3
env = PYTHONIOENCODING=UTF-8
env = DJANGO_SETTINGS_MODULE=vcms.settings_{{ item.name }}
logto = /var/log/vcms/uwsgi_vcms_{{ item.name }}.log
master = true
enable-threads = true
single-interpreter = true
chdir = /vagrant/vcms
module = vcms.wsgi
vacuum = true
uid = root
chmod-socket=666